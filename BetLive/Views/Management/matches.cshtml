<script id="liveGames.html" type="text/ng-template">
    <div>
        <div class="row">
            <div class="col-lg-9">
                <h2 class="heading">Live </h2>
                <p class="heading"><input type="button" class="btn btn-primary" value="Reconnect" ng-click="reconnect()"/> </p>
                <p ng-bind="NoGamesInPlay" ng-show="areNoGamesInPlay"></p>
                <div id="gameTable" >
                    <table class="table table-condensed  oddstable">
                        <thead>
                            <tr><th colspan="5"></th><th colspan="3" class="text-center">Full&nbsp;Time</th><th colspan="3" class="text-center">Under/Over</th><th colspan="3" class="text-center">Double&nbsp;Chance&nbsp;</th><th colspan="3" class="text-center">Next&nbsp;Goal</th></tr>
                            <tr style="background-color:#0D1F35 !important; color: #ffffff;">
                                <th>Match No</th>
                                <th>Min</th>
                                <th>Home</th>
                                <th>Score</th>
                                <th>Away</th>
                                <th class="text-center">1</th>
                                <th class="text-center">X</th>
                                <th class="text-center">2</th>
                                <th class="text-center"><i class="fa fa-sort-down"></i></th>
                                <th class="text-center"></th>
                                <th class="text-center"><i class="fa fa-sort-up"></i></th>
                                <th class="text-center">1</th>
                                <th class="text-center">X</th>
                                <th class="text-center">2</th>
                                <th class="text-center">1</th>
                                <th class="text-center">X</th>
                                <th class="text-center">2</th>
                            </tr>
                        </thead>
                        <tbody id="tbody">
                            <!--<tr class="loading text-center"><td colspan="11">loading...</td></tr>-->

                            <tr  ng-repeat="game in gameRows" data-symbol="{{ game.MatchNo }}">

                                
                                <td data-field="matchId" class="match-code">{{ game.MatchNo }}</td>
                                
                                <td data-field="startTime" class="start-time">{{ game.Minutes }}</td>
                                
                                <td data-field="teamVersus" class="team-versus hidden">{{ game.LocalTeam }} +" vs " +{{ game.AwayTeam}}</td>
                                
                                <td>{{ game.LocalTeam }}</td>
                               
                                <td data-field="liveScores" class="live-scores">{{ game.LocalTeamScore }} - {{ game.AwayTeamScore }}</td>
                                
                                <td>{{ game.AwayTeam }}</td>
                               
                                <td style="padding-bottom: 0px;">
                                   
                                    <input type="button" class="btn btn-sm btn-default FT-1 odd" value="{{ game.FullTimeOdds.HomeWins }}" data-odd-type="Live" data-option-id="7" data-option-name="FT1" data-bet-category="FT1X2" />
                                    
                                </td>
                                
                                <td>
                                   
                                    <input type="button" class="btn btn-sm btn-default FT-X odd" value="{{ game.FullTimeOdds.Draw }}" data-odd-type="Live" data-option-id="7" data-option-name="FTX" data-bet-category="FT1X2" />
                                  
                                </td>
                                
                                <td>
                                   
                                   <input type="button" class="btn btn-sm btn-default FT-2 odd" value="{{ game.FullTimeOdds.AwayWins }}" data-odd-type="Live" data-option-id="7" data-option-name="FT2" data-bet-category="FT1X2" />
                                  
                                </td>
                                
                                <td>
                                   
                                    <input type="button" class="btn btn-sm btn-default odd" value="{{ game.UnderOverOdds.Under }}" data-odd-type="Live" data-option-id="7" data-option-name="U" data-bet-category="U/O" />
                                    
                                </td>                            
                                <td style="padding-top: 14px;">
                                   
                                      <span style="color:#E9860D;">{{ game.UnderOverOdds.ExtraValue }}</span>
                                    
                                </td>
                                
                                <td>
                                   
                                    <input type="button" class="btn btn-sm btn-default odd" value="{{ game.UnderOverOdds.Over }}" data-odd-type="Live" data-option-id="7" data-option-name="O" data-bet-category="U/O" />
                                    
                                </td>

                                             
                                                <td style="padding-bottom: 0px;">                                                   
                                                    <input type="button" class="btn btn-sm btn-default FT-1 odd" value="{{game.DoubleChance.HomeWinsOrDraw}}" data-odd-type="Live" data-option-id="7" data-option-name="DC-1" data-bet-category="Double Chance" />
                                                 
                                                </td>
                                               
                                                <td>
                                                   
                                                    <input type="button" class="btn btn-sm btn-default FT-X odd" value="{{game.DoubleChance.HomeWinsOrAwayWins}}" data-odd-type="Live" data-option-id="7" data-option-name="DC-X" data-bet-category="Double Chance" />
                                                   
                                                </td>
                                              
                                                <td>
                                                   
                                                    <input type="button" class="btn btn-sm btn-default FT-2 odd" value="{{game.DoubleChance.AwayWinsOrDraw}}" data-odd-type="Live" data-option-id="7" data-option-name="DC-2" data-bet-category="Double Chance" />
                                                   
                                                </td>
                                               
                                                <td style="padding-bottom: 0px;">
                                                   
                                                   <input type="button" class="btn btn-sm btn-default FT-1 odd" value="{{game.NextGoalOdds.HomeScores}}" data-odd-type="Live" data-option-id="7" data-option-name="FT1-NG" data-bet-category="FT1X2-NG" />
                                                  
                                                </td>
                                               
                                                <td>
                                                   
                                                   <input type="button" class="btn btn-sm btn-default FT-X odd" value="{{game.NextGoalOdds.Draw}}" data-odd-type="Live" data-option-id="7" data-option-name="FTX-NG" data-bet-category="FT1X2-NG" />
                                                    
                                                </td>
                                               
                                                <td>
                                                   
                                                   <input type="button" class="btn btn-sm btn-default FT-2 odd" value="{{game.NextGoalOdds.AwayScores}}" data-odd-type="Live" data-option-id="7" data-option-name="FT2-NG" data-bet-category="FT1X2-NG" />
                                                   
                                                </td>                                               
                                            
                                            </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-lg-3">

            </div>
        </div>
    </div>
</script>