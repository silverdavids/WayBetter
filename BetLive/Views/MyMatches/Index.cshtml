    <!-- Add your site or application content here -->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" data-ng-controller="indexCtrl">
        <div class="container">
            <div class="navbar-header">
                <button class="btn btn-success navbar-toggle" data-ng-click="navbarExpanded = !navbarExpanded">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </button>
                <a href="#"><img src="@Url.Content("~/Content/Images/betway1.png")" height="50px" width="150px" alt="BETWAY UG" /></a>
                <h1 class="sr-only">BETWAY Sports Betting</h1>
               
                <!--<a class="navbar-brand" href="#/">Home</a>-->
            </div>

            <div class="collapse navbar-collapse" data-collapse="!navbarExpanded">
                <ul class="nav navbar-nav navbar-right" ng-cloak>
                    <li class="btn  btn-warning "><a href="../Live/Games"> live in play</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="#">Welcome:<span ng-bind="authentication.userName"></span></a></li>
                   
                   <!-- <li data-ng-hide="!authentication.isAuth"><a href="#/company">Companies</a></li>-->
                    <li><a href="#/matches">Matches</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="" data-ng-click="logOut()">Logout</a></li>
                    <li data-ng-hide="authentication.isAuth" > <a href= "#/login">Login</a></li>
                    <li data-ng-hide="authentication.isAuth"> <a href="#/signup">Sign Up</a></li>
                    <li>@ViewBag.Balance</li>
                    <li><input type="button" value="Ticket" data-target="#app" data-toggle="offcanvas" class="btn  btn-warning" /></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="offset-margin row ">
        <div class="col-lg-12">
            <div class=" main-div-content">
                <ng-view>
                </ng-view>
            </div>
        </div>
    </div>
    <hr />
    <div id="footer" class="footer">
        <div class="container ">
            <div class="row">
                <div class="col-md-6">
                    <p>With <span class="glyphicon glyphicon-heart"></span> by the team></p>
                </div>
                <div class="col-md-6">
                    <p class="text-muted"> For Bet Way</p>
                </div>
            </div>
